<script>
    function getQueryString(name) {
        var reg = new RegExp("(^|&)" + name + "=([^&]*)(&|$)", "i");
        var r = window.location.search.substr(1).match(reg);
        if (r != null) return unescape(r[2]);
        return null;
    }
    $(function(){
        var params = getQueryString('from');
        if(params == 'tlife' && (navigator.userAgent.indexOf('=>') > -1)) {
            localStorage.setItem('tlife', true);
        }
        $('.global-search-icon').on('click',function(e){
            var str = $('#searchLink').attr('href');
            var keyword= $('.header-search').val() || $('.hd-mob-search').val();
            var link = str;
            if(keyword!=""){
              link=link+"/"+keyword;
          }
          window.location.href = link;
      });

        $('.header-search').on('keydown',function(event){
          if(event.keyCode ==13){
              $('.global-search-icon').trigger('click');
          }
      });


        $('.hd-mob-search').on('keydown',function(event){
            if(event.keyCode ==13){
                $('.global-search-icon').trigger('click');
            }
        });

        $('.cm-header-mob-search-con-btn').on('touchstart',function(e){
            var str = $('#searchLink').attr('data-link');
            var link = str;
            if(keyword!=""){
              link=link+"/"+keyword;
          }
          window.location.href = link;
      });
    })
</script>
</div>
    

